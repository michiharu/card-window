{
  "name": "card-window-dev",
  "private": true,
  "workspaces": [
    "packages/*",
    "docusaurus/*"
  ],
  "scripts": {
    "build": "lerna run build --scope card-window",
    "coverage": "lerna exec --scope card-window -- yarn test --coverage",
    "fix": "lerna run fix --scope card-window",
    "postinstall": "npx sort-package-json",
    "pack": "lerna exec --scope card-window -- npm pack",
    "test": "lerna run test --scope card-window",
    "watch": "lerna run watch --scope card-window"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "lerna": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
